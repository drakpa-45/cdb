	<queryString>
		<![CDATA[SELECT A.*,B.* FROM (SELECT
  a.Application_Number,
  a.CDB_Number,
  a.App_Submission_Date,
  a.App_Approval_Date,
  (SELECT
    Owner_Name
  FROM
    t_reg_appl_owner
   WHERE Application_Number=a.Application_Number AND Nationality IS NULL ) AS Owner_Name,
  (SELECT
    CID_Number FROM t_reg_appl_owner
  WHERE Application_Number=a.Application_Number AND Nationality IS NULL )AS CID_Number,
  (SELECT
    ADDDATE(App_Approval_Date, 1098)
  FROM
    t_registration_con_application
  WHERE Application_Number = a.Application_Number) AS expiry_date
FROM
  t_registration_con_application a
WHERE Application_Number =  $P{applicationNo}) A,
(SELECT GROUP_CONCAT(o.Name SEPARATOR ',') AS pname FROM
    t_owner_dtls o
   WHERE o.CDB_Number IN (SELECT CDB_Number FROM t_registration_con_application app WHERE app.Application_Number=$P{applicationNo}) AND o.Identifier='p')B]]>
	</queryString>
