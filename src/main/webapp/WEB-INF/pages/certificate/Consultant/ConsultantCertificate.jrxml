	<subDataset name="Civil">
		<parameter name="applicationNo" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
  (
    CASE
      WHEN Eng_Services = 'C1'
      THEN 'C1 - Structural'
      WHEN Eng_Services = 'C2'
      THEN 'C2 - Geo-tech Studies'
      WHEN Eng_Services = 'C3'
      THEN 'C3 - Social Studies & Environmental Studies'
      WHEN Eng_Services = 'C4'
      THEN 'C4 - Road & Bridges'
      WHEN Eng_Services = 'C5'
      THEN 'C5 - Irrigation & Hydraulics'
      WHEN Eng_Services = 'C6'
      THEN 'C6 - Construction Management & Site Supervision'
      ELSE 'C7 - Services: Water supply, Sanitation,Sewerage, Solid Waste and Surveying'
    END
  ) Eng_Services,
  Eng_Services_Value
FROM
  t_engineering_services WHERE Application_Number = $P{applicationNo}]]>
		</queryString>
		<field name="Eng_Services" class="java.lang.String"/>
		<field name="Eng_Services_Value" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Electrical">
		<parameter name="applicationNo" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
  (
    CASE
      WHEN Eleng_Services = 'E1'
      THEN 'E1 - Rural Electrification &
        Transmission Line
'
      WHEN Eleng_Services = 'E2'
      THEN 'E2 - Urban Electrification &
        Communication and Scada
'
      WHEN Eleng_Services = 'E3'
      THEN 'E3 - Sub-Station'
      WHEN Eleng_Services = 'E4'
      THEN 'E4 - Major Hydro Power Construction'
      WHEN Eleng_Services = 'E5'
      THEN 'E5 - Small Hydro Power Construction'
      WHEN Eleng_Services = 'E6'
      THEN 'E6 - Mini Hydro Power Construction'
      ELSE 'E7 - House Wiring'
    END
  ) Eleng_Services,
  Eleng_Services_Value
FROM
  t_elec_engineering_services WHERE Application_Number = $P{applicationNo}]]>
		</queryString>
		<field name="Eleng_Services" class="java.lang.String"/>
		<field name="Eleng_Services_Value" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Architect">
		<parameter name="applicationNo" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
  (
    CASE
      WHEN Arch_Services = 'A1'
      THEN 'A1 - Architectural and Interior Design'
      WHEN Arch_Services = 'A2'
      THEN 'A2 - Urban Planning'
      ELSE 'A3 - Landscaping and Site Development'
    END
  ) Arch_Services,
  Arch_Services_Value
FROM
  t_architectural_services
  WHERE Application_Number = $P{applicationNo}]]>
		</queryString>
		<field name="Arch_Services" class="java.lang.String"/>
		<field name="Arch_Services_Value" class="java.lang.String"/>
	</subDataset>
