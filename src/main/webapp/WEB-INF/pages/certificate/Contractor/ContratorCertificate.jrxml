	<queryString>
		<![CDATA[SELECT
   a.`CDBNo`,
  a.`RegistrationApprovedDate`,
  a.`ReRegistrationDate`,
  a.`RegistrationExpiryDate`,
  CONCAT('(',GROUP_CONCAT(f.`CIDNo`),')') cid,GROUP_CONCAT(f.`Name`) contractorName,a.`NameOfFirm`,
  IF(d.`NameEn` IS NULL,'',d.`NameEn`) dName,
  IF((SELECT `Name` FROM `cmncontractorclassification`  WHERE `Id`=(SELECT CmnApprovedClassificationId FROM crpcontractorworkclassificationfinal WHERE CmnProjectCategoryId='6cd737d4-a2b7-11e4-b4d2-080027dcfac6' AND CrpContractorFinalId=a.`Id`)) IS NULL,'-',(SELECT `Name` FROM `cmncontractorclassification`  WHERE `Id`=(SELECT CmnApprovedClassificationId FROM crpcontractorworkclassificationfinal WHERE CmnProjectCategoryId='6cd737d4-a2b7-11e4-b4d2-080027dcfac6' AND CrpContractorFinalId=a.`Id`)))w1,
  IF((SELECT `Name` FROM `cmncontractorclassification`  WHERE `Id`=(SELECT CmnApprovedClassificationId FROM crpcontractorworkclassificationfinal WHERE CmnProjectCategoryId='8176bd2d-a2b7-11e4-b4d2-080027dcfac6' AND CrpContractorFinalId=a.`Id`)) IS NULL,'-',(SELECT `Name` FROM `cmncontractorclassification`  WHERE `Id`=(SELECT CmnApprovedClassificationId FROM crpcontractorworkclassificationfinal WHERE CmnProjectCategoryId='8176bd2d-a2b7-11e4-b4d2-080027dcfac6' AND CrpContractorFinalId=a.`Id`))) w2,
  IF((SELECT `Name` FROM `cmncontractorclassification`  WHERE `Id`=(SELECT CmnApprovedClassificationId FROM crpcontractorworkclassificationfinal WHERE CmnProjectCategoryId='8afc0568-a2b7-11e4-b4d2-080027dcfac6' AND CrpContractorFinalId=a.`Id`)) IS NULL,'-',(SELECT `Name` FROM `cmncontractorclassification`  WHERE `Id`=(SELECT CmnApprovedClassificationId FROM crpcontractorworkclassificationfinal WHERE CmnProjectCategoryId='8afc0568-a2b7-11e4-b4d2-080027dcfac6' AND CrpContractorFinalId=a.`Id`))) w3,
  IF((SELECT `Name` FROM `cmncontractorclassification`  WHERE `Id`=(SELECT CmnApprovedClassificationId FROM crpcontractorworkclassificationfinal WHERE CmnProjectCategoryId='9090a82a-a2b7-11e4-b4d2-080027dcfac6' AND CrpContractorFinalId=a.`Id`)) IS NULL,'-',(SELECT `Name` FROM `cmncontractorclassification`  WHERE `Id`=(SELECT CmnApprovedClassificationId FROM crpcontractorworkclassificationfinal WHERE CmnProjectCategoryId='9090a82a-a2b7-11e4-b4d2-080027dcfac6' AND CrpContractorFinalId=a.`Id`))) w4
FROM
  `crpcontractorfinal` a
  LEFT JOIN `crpcontractorhumanresourcefinal` f ON f.`CrpContractorFinalId`=a.`Id`
LEFT JOIN `cmndzongkhag` d ON d.`Id`=a.`CmnDzongkhagId`
WHERE a.`CDBNo`=$P{applicationNo} AND f.`IsPartnerOrOwner`=1]]>
	</queryString>
